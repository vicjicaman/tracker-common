  "scripts": {
    "clean:server": "rm -Rf ./dist/server*",
    "clean:web": "rm -Rf ./dist/web*",
    "build:prod:server": "yarn clean:server && webpack --config webpack.config.server.js --env.production ",
    "build:dev:server": "yarn clean:server && webpack --config webpack.config.server.js --env.development ",
    "build:prod:web": "yarn clean:web && webpack --config webpack.config.web.js --env.production ",
    "build:dev:web": "yarn clean:web && webpack --config webpack.config.web.js --env.development ",
    "build:prod:resources": "webpack --config webpack.config.resources.js --env.production ",
    "build:dev:resources": "webpack --config webpack.config.resources.js --env.development ",
    "analyze": "webpack --config webpack.config.js --env.analyze",
    "build:dev": "run-p \"build:dev:*\" ",
    "build:prod": "run-p \"build:prod:*\" ",
    "start:dev": "run-p \"build:dev:* --watch\" ",
    "start:prod": "run-p \"build:prod:* --watch\" ",
    "build:watch:dev": "run-p \"build:dev:* --watch\" ",
    "build:watch:prod": "run-p \"build:prod:* --watch\" "
  },
